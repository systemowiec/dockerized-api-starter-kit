{
    "name": "Starter",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "symfony/symfony": "^3.2",
        "symfony/serializer": "^3.2",
        "sensio/framework-extra-bundle": "^3.0",
        "monolog/monolog": "^1.22",
        "ramsey/uuid": "^3.7",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "behat/behat": "^3.3",
        "phpspec/phpspec": "^3.2",
        "phpspec/nyan-formatters": "^2.0",
        "behat/symfony2-extension": "^2.1",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "": "src/",
            "Application\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Context\\": "features/bootstrap/Context"
        }
    },
    "config": {
        "platform": {
            "php": "7.0"
        },
        "bin-dir": "bin"
    }
}
